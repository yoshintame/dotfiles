{
  // APPEARANCE
  "workbench.colorTheme": "True Vibrant (Xcode tokens)",
  "workbench.iconTheme": "pcode-icon-theme-dark-spesifik",

  // Apc
  "apc.electron": {
    "backgroundColor": "rgba(0, 0, 0, 0)",
    "frame": false,
    "vibrancy": "under-window",
    "visualEffectState": "active"
  },
  "apc.header": {
    "height": 34
  },
  "apc.listRow": {
    "height": 21
  },
  "apc.menubar.compact": true,
  "apc.sidebar.titlebar": {
    "fontSize": 12,
    "height": 38
  },
  "apc.statusBar": {
    "height": 24
  },
  "apc.imports": [
    "${userHome}/.cursor/extensions/true-vibrant-vscode-theme/themes/true-vibrant-color-theme.css"
  ],

  // Fonts
  "apc.font.family": "Cascadia Code",
  "editor.fontFamily": "FiraCode Nerd Font",
  "editor.fontLigatures": false,
  "editor.fontSize": 12,
  "editor.fontWeight": "600",
  "editor.lineHeight": 18,
  "terminal.integrated.fontFamily": "'Hack Nerd Font'",
  "terminal.integrated.fontSize": 11,
  "terminal.integrated.fontWeight": "300",
  "terminal.integrated.lineHeight": 1.23,
  "workbench.fontAliasing": "antialiased",

  // Layout
  "breadcrumbs.enabled": false,
  "editor.tabSize": 4,
  "explorer.compactFolders": false,
  "files.simpleDialog.enable": true,
  "git-graph.commitDetailsView.location": "Docked to Bottom",
  "window.commandCenter": false,
  "window.density.editorTabHeight": "compact",
  "window.dialogStyle": "custom",
  "window.titleBarStyle": "native",
  "workbench.activityBar.location": "hidden",
  "workbench.layoutControl.enabled": false,
  "workbench.LayoutControl.enabled": false,
  "editor.lineNumbers": "off",

  // Cursor
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.cursorStyle": "line-thin",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.cursorStyle": "line",

  // Decorations
  "workbench.tree.indent": 20,
  "workbench.tree.renderIndentGuides": "always",

  // Scroll and minimap
  "editor.scrollbar.horizontalScrollbarSize": 8,
  "editor.scrollbar.verticalScrollbarSize": 8,
  "editor.minimap.enabled": false,
  "editor.minimap.scale": 1,
  "editor.minimap.renderCharacters": false,
  "editor.minimap.maxColumn": 120,
  "editor.minimap.showSlider": "always",
  "editor.stickyScroll.enabled": false,
  "editor.stickyScroll.scrollWithEditor": false,
  "workbench.tree.enableStickyScroll": false,

  // UX
  "debug.onTaskErrors": "debugAnyway",
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.accessibilitySupport": "off",
  "editor.bracketPairColorization.enabled": false,
  "editor.bracketPairColorization.independentColorPoolPerBracketType": false,
  "editor.cursorSurroundingLines": 5,
  "editor.detectIndentation": false,
  "editor.find.addExtraSpaceOnTop": false,
  "editor.foldingImportsByDefault": true,
  "editor.formatOnSave": true,
  "editor.gotoLocation.multipleDeclarations": "gotoAndPeek",
  "editor.gotoLocation.multipleDefinitions": "gotoAndPeek",
  "editor.gotoLocation.multipleImplementations": "gotoAndPeek",
  "editor.gotoLocation.multipleReferences": "gotoAndPeek",
  "editor.gotoLocation.multipleTypeDefinitions": "gotoAndPeek",
  "editor.guides.bracketPairs": "active",
  "editor.hover.delay": 200,
  "editor.hover.sticky": true,
  "editor.inlineSuggest.enabled": true,
  "editor.linkedEditing": true,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.overviewRulerBorder": false,
  "editor.renderWhitespace": "boundary",
  "editor.suggest.insertMode": "replace",
  "editor.suggestFontSize": 14,
  "editor.suggestSelection": "first",
  "editor.unicodeHighlight.invisibleCharacters": false,
  "editor.wordSeparators": "`~!@#%^&*()=+[{]}\\|;:'\",.<>/?",
  "editor.wordWrap": "bounded",
  "editor.wordWrapColumn": 1000,
  "emmet.showSuggestionsAsSnippets": true,
  "emmet.triggerExpansionOnTab": false,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "workbench.editor.limit.enabled": true,
  "workbench.editor.limit.perEditorGroup": true,
  "workbench.editor.limit.value": 6,
  "workbench.list.smoothScrolling": true,
  "workbench.startupEditor": "none",
  "workbench.tree.expandMode": "singleClick",

  // Format, lint and LSP
  "editor.formatOnPaste": true,
  "files.autoSave": "afterDelay",
  "editor.codeActionsOnSave": {
    "source.addMissingImports": "explicit",
    "source.organizeImports": "explicit",
    "source.fixAll": "never",
    "source.fixAll.eslint": "explicit"
  },
  "files.eol": "\n",
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "eslint.codeAction.showDocumentation": {
    "enable": true
  },
  "eslint.quiet": true,
  "eslint.run": "onSave",
  "eslint.validate": [
    "javascript",
    "typescript",
    "javascriptreact",
    "typescriptreact",
    "vue",
    "html",
    "markdown",
    "json",
    "jsonc",
    "json5"
  ],
  "typescript.disableAutomaticTypeAcquisition": true,
  "typescript.tsdk": "./node_modules/typescript/lib",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "prettier.useEditorConfig": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[prisma]": {
    "editor.defaultFormatter": "Prisma.prisma"
  },
  "[go]": {
    "editor.defaultFormatter": "golang.go"
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.autopep8"
  },
  "sortJSON.orderOverride": [
    "name",
    "version",
    "main",
    "module",
    "types",
    "typings",
    "files",
    "publishConfig",
    "repository",
    "scripts",
    "prefix",
    "description",
    "body"
  ],
  "sortJSON.orderUnderride": [
    "resolutions",
    "dependencies",
    "devDependencies",
    "peerDependencies",
    "cSpell.userWords"
  ],

  // Git
  "git.enableCommitSigning": true,
  "git.autofetch": true,
  "git.autoStash": true,
  "git.confirmSync": false,
  "git.openRepositoryInParentFolders": "always",
  "github.copilot.enable": {
    "*": true,
    "markdown": true,
    "plaintext": false,
    "yaml": false
  },

  // Exclude
  "files.exclude": {
    "**/*.cssc": true,
    "**/*.log.*": true,
    "**/*.min.*": true,
    "**/*.scssc": true,
    "**/.deploy_git": true,
    "**/.DS_Store": true,
    "**/.git": false,
    "**/.hg": true,
    "**/.svn": true,
    "**/.trunk/*actions/": true,
    "**/.trunk/*logs/": true,
    "**/.trunk/*notifications/": true,
    "**/.trunk/*out/": true,
    "**/.trunk/*plugins/": true,
    "**/cache": true,
    "**/db.json": true,
    "**/dist": true,
    "**/lcov-report": true,
    "**/node_modules": false,
    "**/public": true,
    "**/rethinkdb_data": true,
    "**/test_assets": true,
    "**/tmp": true
  },
  "search.exclude": {
    "**/.git": true,
    "**/.github": true,
    "**/.nuxt": true,
    "**/.output": true,
    "**/.pnpm": true,
    "**/.vscode": true,
    "**/.yarn": true,
    "**/dist/**": true,
    "**/logs": true,
    "**/out/**": true,
    "**/package-lock.json": true,
    "**/pnpm-lock.yaml": true,
    "**/yarn.lock": true,
    "**/tmp": true,
    "**/*.code-search": true,
    "**/*.jpeg": true,
    "**/*.jpg": true,
    "**/*.png": true,
    "**/*.svg": true,
    "**/.gitattributes": true,
    "**/.gitignore": true,
    "**/.vscodeignore": true,
    "**/bower_components": true,
    "**/node_modules": true
  },
  "files.watcherExclude": {
    "**/.trunk/*actions/": true,
    "**/.trunk/*logs/": true,
    "**/.trunk/*notifications/": true,
    "**/.trunk/*out/": true,
    "**/.trunk/*plugins/": true
  },

  // Terminal
  "terminal.external.osxExec": "warp.app",
  "terminal.integrated.env.osx": {
    "FIG_NEW_SESSION": "1"
  },
  "terminal.integrated.gpuAcceleration": "off",
  "terminal.integrated.tabs.enabled": true,
  "terminal.integrated.persistentSessionReviveProcess": "never",

  // System and Security
  "extensions.autoUpdate": "onlyEnabledExtensions",
  "extensions.ignoreRecommendations": true,
  "remote.autoForwardPortsSource": "hybrid",
  "security.promptForLocalFileProtocolHandling": false,
  "security.workspace.trust.enabled": false,
  "update.mode": "manual",
  "update.showReleaseNotes": false,

  // OTHER EXTENSIONS
  "errorLens.fontStyleItalic": true,
  "i18n-ally.autoDetection": false,
  "i18n-ally.displayLanguage": "en",
  "i18n-ally.ignoredLocales": [],
  "iconify.annotations": true,
  "iconify.inplace": true,
  "projectManager.git.baseFolders": ["~"],
  "projectManager.sortList": "Recent",
  "svg.preview.mode": "svg",
  "tailwind-fold.autoFold": false,
  "tailwind-fold.foldedText": "â€¦",
  "tailwind-fold.foldedTextBackgroundColor": "#1f1f1f",
  "tailwind-fold.foldedTextColor": "#7e7e7e",
  "tailwind-fold.showTailwindImage": false,
  "tailwindCSS.classAttributes": [
    "class",
    "className",
    "ngClass",
    "variants",
    "variant"
  ],

